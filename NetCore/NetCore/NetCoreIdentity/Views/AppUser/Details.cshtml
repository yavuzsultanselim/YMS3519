@model AppUser
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="card">
        <div class="card-header">
            <h2>@Model.UserName</h2>
        </div>
        <div class="card-body">
            <ul>
                <li>@Model.Email</li>
                <li>@Model.PhoneNumber</li>
                <li>@Model.BirthDate</li>
            </ul>
        </div>
    </div>
</div>

